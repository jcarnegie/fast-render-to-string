var render<%= componentName %> = function( /* props, children... */ ) {
    this.getInitialProps = <%= getInitialPropsFn %>;
    this.getInitialState = <%= getInitialStateFn %>;
    var children = [];
    this.props = r.merge(this.getInitialProps(), arguments[0]);
    this.state = this.getInitialState();
    for(var i = 1; i < arguments.length; i++)
        children[i] = arguments[i];
    <%= renderBody %>
}